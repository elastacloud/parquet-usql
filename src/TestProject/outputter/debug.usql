@a  = 
    SELECT * FROM 
        (VALUES
            ("Contoso", 1500.0, DateTime.UtcNow),
            ("Woodgrove", 2700.0, DateTime.UtcNow),
            ("Contoso", 1500.0, DateTime.UtcNow),
            ("Woodgrove", 2700.0, DateTime.UtcNow)

        ) AS 
              D( customer, amount, date );
OUTPUT @a
    TO "/test1.parquet"
    USING new Parquet.Adla.Outputter.ParquetOutputter();


